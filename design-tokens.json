{
  "breakpoints": {
    "lg": {
      "$type": "breakpoint",
      "$value": "1200px"
    },
    "md": {
      "$type": "breakpoint",
      "$value": "900px"
    },
    "sm": {
      "$type": "breakpoint",
      "$value": "600px"
    },
    "xl": {
      "$type": "breakpoint",
      "$value": "1536px"
    },
    "xs": {
      "$type": "breakpoint",
      "$value": "0px"
    }
  },
  "colors": {
    "neutrals": {
      "darkGrey": {
        "$type": "color",
        "$value": "#616161",
        "$description": "Darker grey for contrast, subtitles, and UI depth."
      },
      "darkestGrey": {
        "$type": "color",
        "$value": "#424242",
        "$description": "Deep, nearly black grey for high contrast text and overlays."
      },
      "lightGrey": {
        "$type": "color",
        "$value": "#E0E0E0",
        "$description": "Soft grey for UI elements, borders, and secondary backgrounds."
      },
      "lightestGrey": {
        "$type": "color",
        "$value": "#F9F9F9",
        "$description": "Very light grey, ideal for backgrounds and subtle dividers."
      },
      "mediumGrey": {
        "$type": "color",
        "$value": "#9E9E9E",
        "$description": "Balanced grey for body text, icons, and neutral accents."
      },
      "mediumLightGrey": {
        "$type": "color",
        "$value": "#C4C4C4",
        "$description": "Neutral grey for less prominent text and input fields."
      }
    },
    "primary": {
      "carnation": {
        "$type": "color",
        "$value": "#FA5053",
        "$description": "Bold hue for energetic highlights, promotional banners, and attention-grabbing elements."
      },
      "peep": {
        "$type": "color",
        "$value": "#F9E8EA",
        "$description": "Soft background color for sections, product labels, and branding."
      },
      "thunderbird": {
        "$type": "color",
        "$value": "#BD5319",
        "$description": "Main brand color for logos, CTA buttons, and key highlights."
      }
    },
    "secondary": {
      "cashmere": {
        "$type": "color",
        "$value": "#E8C3B0",
        "$description": "Muted background/accent color for subtle overlays and packaging details."
      },
      "espresso": {
        "$type": "color",
        "$value": "#69321A",
        "$description": "Deep contrast color for text, headings, and packaging outlines."
      }
    },
    "tertiary": {
      "barley": {
        "$type": "color",
        "$value": "#A98D65",
        "$description": "Subtle background and border color reinforcing an organic aesthetic."
      },
      "cannon": {
        "$type": "color",
        "$value": "#914C5E",
        "$description": "Accent color for highlights, social media templates, and limited-edition packaging."
      }
    }
  },
  "spacing": {
    "3xl": {
      "$type": "spacing",
      "$value": "48px"
    },
    "4xl": {
      "$type": "spacing",
      "$value": "64px"
    },
    "5xl": {
      "$type": "spacing",
      "$value": "80px"
    },
    "6xl": {
      "$type": "spacing",
      "$value": "96px"
    },
    "lg": {
      "$type": "spacing",
      "$value": "24px"
    },
    "md": {
      "$type": "spacing",
      "$value": "16px"
    },
    "sm": {
      "$type": "spacing",
      "$value": "12px"
    },
    "xl": {
      "$type": "spacing",
      "$value": "32px"
    },
    "xs": {
      "$type": "spacing",
      "$value": "8px"
    },
    "xxl": {
      "$type": "spacing",
      "$value": "40px"
    },
    "xxs": {
      "$type": "spacing",
      "$value": "4px"
    }
  },
  "transitions": {
    "$type": "transition",
    "durations": {
      "$type": "duration",
      "instant": {
        "$type": "duration",
        "$value": "0ms"
      },
      "long": {
        "$type": "duration",
        "$value": "600ms"
      },
      "medium": {
        "$type": "duration",
        "$value": "300ms"
      },
      "short": {
        "$type": "duration",
        "$value": "100ms"
      }
    },
    "easingFunctions": {
      "$type": "cubicBezier",
      "accelerate": {
        "$type": "cubicBezier",
        "$value": [
          0.5,
          0,
          1,
          1
        ]
      },
      "decelerate": {
        "$type": "cubicBezier",
        "$value": [
          0,
          0,
          0.5,
          1
        ]
      }
    },
    "emphasis": {
      "$type": "transition",
      "$value": {
        "delay": "{transitions.durations.instant}",
        "duration": "{transitions.durations.medium}",
        "timingFunction": "{transitions.easingFunctions.accelerate}"
      }
    },
    "fade": {
      "$type": "transition",
      "$value": {
        "delay": "{transitions.durations.instant}",
        "duration": "{transitions.durations.long}",
        "timingFunction": "{transitions.easingFunctions.decelerate}"
      }
    }
  },
  "typography": {
    "cabin": {
      "$type": "fontFamily",
      "$value": "'Cabin', sans-serif"
    },
    "cantarell": {
      "fontFamily": {
        "$type": "fontFamily",
        "$value": "'Cantarell', serif"
      },
      "fontSize": {
        "subheadline": {
          "$type": "fontSize",
          "$value": "18px"
        },
        "sublogo": {
          "$type": "fontSize",
          "$value": "36px"
        }
      },
      "fontWeight": {
        "bold": {
          "$type": "fontWeight",
          "$value": "bold"
        },
        "normal": {
          "$type": "fontWeight",
          "$value": "normal"
        }
      }
    },
    "youngSerif": {
      "fontFamily": {
        "$type": "fontFamily",
        "$value": "'Young Serif', serif"
      },
      "fontSize": {
        "logo": {
          "$type": "fontSize",
          "$value": "128px"
        }
      },
      "fontWeight": {
        "normal": {
          "$type": "fontWeight",
          "$value": "normal"
        }
      }
    },
    "yrsa": {
      "fontFamily": {
        "$type": "fontFamily",
        "$value": "'Yrsa', serif"
      },
      "fontSize": {
        "heading": {
          "$type": "fontSize",
